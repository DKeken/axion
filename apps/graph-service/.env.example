# Database (отдельная БД для graph-service)
DATABASE_URL=postgresql://axion:axion_password@localhost:5433/axion_graph

# Redis (Service Discovery)
REDIS_URL=redis://:axion_redis_password@localhost:6379

# Kafka (Event Bus для CQRS и Event Sourcing)
KAFKA_BROKERS=localhost:9092
