{
  "protoc": {
    "options": ["--proto_path=${workspaceFolder}/packages/contracts/proto"],
    "compile_on_save": false
  },
  "protoc.path": "",
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3",
